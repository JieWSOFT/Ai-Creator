@import "./assets/font/iconfont.wxss";

.flex{
  display: flex;
}
.flex-1{
  flex: 1;
}
.col-column{
  flex-direction: column;
}
.items-center{
  align-items: center;
}
.justify-center{
  justify-content: center;
}
.justify-between{
  justify-content: space-between;
}
.flex-a-c{
  display: flex;
  align-items: center;
}
.flex-c{
  display: flex;
  align-items: center;
  justify-content: center;
}
.box-border{
  box-sizing: border-box;
}
.overflow-hidden{
  overflow: hidden;
}

